{
  "name": "stylex-variants",
  "version": "0.0.2",
  "author": "Levin Uncu <levin.uncu@pip.de> (https://pip.de)",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify && pnpm dlx publint",
    "check": "tsc src/index.ts --noEmit --skipLibCheck",
    "format": "prettier --write --cache ./src/**/*.ts",
    "format:check": "prettier --check --cache ./src/**/*.ts",
    "lint": "eslint ./src --cache --cache-file=node_modules/.cache/.eslintcache",
    "test": "vitest run"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vite-plugin-stylex": "^0.8.2",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@stylexjs/stylex": "^0.5.1"
  }
}
